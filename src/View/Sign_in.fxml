<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="200.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LoginController">
   <children>
      <GridPane alignment="CENTER" layoutX="15.0" layoutY="35.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="153.0" minWidth="10.0" prefWidth="78.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="246.0" minWidth="10.0" prefWidth="242.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Login" />
            <Label text="Password" GridPane.rowIndex="1" />
            <TextField fx:id="loginText" prefHeight="31.0" prefWidth="211.0" GridPane.columnIndex="1" />
            <PasswordField fx:id="passwordText" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <HBox layoutX="161.0" layoutY="155.0" spacing="15.0" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0">
         <children>
            <Button layoutX="161.0" layoutY="155.0" mnemonicParsing="false" onAction="#HandleReg" text="Registration" />
            <Button layoutX="270.0" layoutY="155.0" mnemonicParsing="false" onAction="#HandleEnter" text="Enter" />
         </children>
      </HBox>
   </children>
</AnchorPane>
